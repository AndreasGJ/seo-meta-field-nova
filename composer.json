{
    "name": "gwd/seo-meta-nova-field",
    "description": "A Laravel Nova field which adds all SEO related meta fields to an Resource.",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/AndreasGJ/seo-meta-field-nova",
    "authors": [
        {
            "name": "Andreas Gade Jensen",
            "email": "andreasgadej@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "keywords": [
        "laravel",
        "nova"
    ],
    "require": {
        "php": ">=7.3.0",
        "laravel/nova": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Gwd\\SeoMeta\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gwd\\SeoMeta\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false
}
